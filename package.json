{
  "name": "anytv-node-mysql",
  "version": "0.0.36",
  "description": "node mysql helper",
  "main": "index.js",
  "dependencies": {
    "mysql": "^2.9.0"
  },
  "devDependencies": {
    "babel": "^6.1.18",
    "babel-preset-es2015": "^6.1.18",
    "blanket": "^1.1.9",
    "chai": "^3.4.1",
    "coveralls": "^2.11.4",
    "esdoc": "^0.4.3",
    "mocha": "^2.3.3",
    "mocha-lcov-reporter": "^1.0.0"
  },
  "scripts": {
    "test": "make test",
    "coverage": "export NODE_ENV=test && mocha --require blanket -R html-cov --recursive -t 5000 -s 100 > lib-cov/coverage.html",
    "test-dev": "export NODE_ENV=test && mocha --watch --recursive -R spec -t 5000 -s 100"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/anyTV/anytv-node-mysql.git"
  },
  "keywords": [
    "mysql"
  ],
  "author": "anyTV",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/anyTV/anytv-node-mysql/issues"
  },
  "homepage": "https://github.com/anyTV/anytv-node-mysql",
  "config": {
    "blanket": {
      "pattern": [
        "lib"
      ],
      "data-cover-never": [
        "node_modules",
        "test",
        "src",
        "doc",
        "coverage"
      ]
    }
  }
}
